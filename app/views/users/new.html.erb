<h1>Register a New User</h1>
<%= form_with url: "/users", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, required: true  %><br>


  <%= f.label :email %>
  <%= f.text_field :email, required: true %><br>
  <%= f.label :password %>
  <%= f.text_field :password, required: true %><br>
  <%= f.label :reenter_password %>
  <%= f.text_field :password_confirmation, required: true %><br>

  <%= f.submit %>
<% end %>
